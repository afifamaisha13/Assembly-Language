.model small
.stack 100h
.data 
a db 0ah,0dh,'$'
m db 'My name is Maisha. My ID is:IT22005',0ah,0dh,'$'
.code
main proc
    mov ax,@data
    mov ds,ax
    
    lea dx,m
    mov ah,09h
    int 21h
    
    mov ah,2
    mov dl,'?'
    int 21h
    
    mov ah,1
    int 21h
    mov bl,al
    int 21h
    mov cl,al
    
    lea dx,a
    mov ah,09h
    int 21h
    
    cmp bl,cl
    jg switch
    jmp result
    
   switch:
   xchg bl,cl
   
   result:
   mov ah,2
   mov dl,bl
   int 21h
   mov dl,cl
   int 21h
   
   exit:
   mov ah,4ch
   int 21h
   main endp
end main

